<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="colorControlHighlight">#A1887F</item>
    </style>

    <style name="topButton">
        <item name="android:background">#795548</item>
    </style>

    <style name="buttonback">
        <item name="android:background">@drawable/borderless</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="keyback">
        <item name="android:background">#A1887F</item>
    </style>
</resources>